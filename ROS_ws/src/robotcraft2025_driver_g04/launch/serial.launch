<!-- Launch the node to communicate with the serial port -->
<launch>
  <node pkg="rosserial_python" type="serial_node.py" name="serial_node">
    <param name="port" value="/dev/ttyACM0"/>
    <param name="baud" value="57600"/>
  </node>
  <node pkg="robotcraft2025_driver_g04" type="main.py" name="pose_and_ir_node" output="screen"/>
  
</launch>